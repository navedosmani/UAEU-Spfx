        }
      ]
    },
    "events-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/events/EventsWebPart.js",
          "manifest": "./src/webparts/events/EventsWebPart.manifest.json"
        }
      ]
    },
    "important-links-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/importantLinks/ImportantLinksWebPart.js",
          "manifest": "./src/webparts/importantLinks/ImportantLinksWebPart.manifest.json"
        }
      ]
