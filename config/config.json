    "research-innovation-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/researchInnovation/ResearchInnovationWebPart.js",
          "manifest": "./src/webparts/researchInnovation/ResearchInnovationWebPart.manifest.json"
        }
      ]
    },
    "social-wall-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/socialWall/SocialWallWebPart.js",
          "manifest": "./src/webparts/socialWall/SocialWallWebPart.manifest.json"
        }
      ]
